<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <% if (categories && categories.length > 0) { %>
    <ul>
      <% categories.forEach((category) => { %>
      <li>
        <a href="/categories/<%= category.id %>"> <%= category.genre %> </a>
      </li>
      <% }) %> <% } %>
    </ul>
  </body>
</html>
